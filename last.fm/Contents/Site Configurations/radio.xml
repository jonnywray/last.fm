<?xml version="1.0" encoding="UTF-8"?>
<site site="http://.*last.fm/listen" plugin="http://.*last.fm/webclient/.*swf"
	initialState="playing" version="1.0">

	<crop x="0" y="0" width="0" height="0" />
	
	<state name="playing">
		<event>
			<condition>
				<command name="pause" />
			</condition>
			<action>
				<click x="33" y="33" />
				<goto state="paused" />
			</action>
		</event>
	</state>

	<state name="paused">
		<event>
			<condition>
				<command name="play" />
			</condition>
			<action>
				<click x="33" y="33" />
				<goto state="playing" />
			</action>
		</event>
	</state>
</site>
